{
  "name": "tsdoc-generator",
  "version": "0.1.0",
  "description": "TSDoc Generator",
  "main": "dist/parser.js",
  "type": "module",
  "bin": {
    "gen-reference-md": "./dist/bin/gen-reference-md.js"
  },
  "files": [
    "dist/*.js"
  ],
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "tsc",
    "postbuild": "chmod +x ./dist/bin/*",
    "prepack": "npm run build",
    "test": "vitest"
  },
  "keywords": [
    "TSDoc",
    "generator"
  ],
  "author": "sacdenoeuds.dev@gmail.com",
  "license": "MIT",
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.15.1",
    "cac": "^6.7.14",
    "ts-morph": "^26.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "tsx": "^4.20.3",
    "vitest": "^3.2.4"
  }
}
